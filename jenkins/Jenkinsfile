pipeline {
    agent any 
    stages {
        stage('TEST') {
            steps {
                sh 'echo Hello SUCCESS'
            }
        }
    }
    post {
        always {
            echo "verbose always"
        }
        success {
            echo "verbose success"
        }
        failure {
            echo "verbose failure"
        }
        unstable {
            echo "verbose unstable"
        }
        changed {
            echo "verbose changed!"
        }
    }
}


