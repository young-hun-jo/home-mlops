pipeline {
    agent any
    
    environment {
        APP_NAME = 'tabular-diabetes-regressor'
    }

    stages {
        stage('Moving directory') {
            steps {
                sh 'cd serving/${APP_NAME} && pwd && ls -l'
            },
            steps {
                sh 'pwd'
            }
        }
        stage('Check pwd') {
            steps {
                sh 'echo $USER'
            }
        }
    }
}
